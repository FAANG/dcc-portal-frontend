{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "data.faang.org",
      "urls": ["http://data.faang.org/api/**"],
      "cacheConfig": {
        "maxSize": 3,
        "maxAge": "7d",
        "strategy": "performance"
      }
    }
  ]
}
